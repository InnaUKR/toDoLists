
<div class="row">
     <div id="header">
      <h1 id="main-header" class="text-uppercase text-center">Simple TODO Lists</h1>
      <h4 class="text-uppercase text-center">From Ruby Garage</h4>
    </div>


<div class="row">
  <div class="" id="projects"><%= render partial: 'projects/project', collection: @projects%>
  </div>
</div>


<div class="row">
  <div class="" id="project-form"  style="display:none;"></div>
</div>

  <div class="text-center">
    <%= link_to new_project_path, remote: true do %>
      <button class="btn btn-primary"><i class="glyphicon glyphicon-plus"></i>Add TODO List</button>
    <% end %>
  </div>
</div>
